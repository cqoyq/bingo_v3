<?xml version="1.0" encoding="utf-8"?>
<application>
    <tcp_receiver>
        <ip>0.0.0.0</ip>
        <port>17015</port>
    </tcp_receiver>
    <rabbitmq_receiver>
        <type>4</type>
        <ip>127.0.0.1</ip>						
        <port>5672</port>							
        <username>guest</username>		
        <pwd>guest</pwd>						
        <host>/</host>
        <exchange>test_routing_exchange</exchange>								
        <routingkey>t1,t2</routingkey>		
    </rabbitmq_receiver>
    <rabbitmq_sendor>
        <list>
            <item type="4" ip="127.0.0.1" port="5672" username="guest" pwd="guest" host="/" exchange="test_routing_exchange">
                <routingkey key="t1">
                    test1
                </routingkey>
                <routingkey key="t2">
                    test2
                </routingkey>
            </item>
        </list>
    </rabbitmq_sendor>
    <rbmq_log_visitor>
        <rabbitmq_key>logger</rabbitmq_key>
        <log_level>debug</log_level>				
    </rbmq_log_visitor>
</application>
